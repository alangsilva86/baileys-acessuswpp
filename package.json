{
  "name": "baileys-acessuswpp",
  "version": "1.0.0",
  "description": "WhatsApp API using Baileys with web dashboard",
  "type": "module",
  "main": "dist/src/server.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node dist/src/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "npm run tailwind:build && tsc -p tsconfig.json",
    "tailwind:build": "tailwindcss -i ./public/styles/tailwind.css -o ./public/assets/app.css --minify",
    "example:poll-webhook": "tsx src/examples/pollWebhookServer.ts",
    "test": "tsx --test \"test/**/*.test.ts\""
  },
  "dependencies": {
    "@whiskeysockets/baileys": "^7.0.0-rc.6",
    "axios": "^1.7.7",
    "bullmq": "^5.7.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "https-proxy-agent": "^7.0.6",
    "ioredis": "^5.8.2",
    "pino": "^9.12.0",
    "qrcode": "^1.5.4",
    "sharp": "^0.33.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/helmet": "^0.0.48",
    "autoprefixer": "^10.4.21",
    "nodemon": "^3.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  },
  "keywords": [
    "whatsapp",
    "baileys",
    "api",
    "bot"
  ],
  "author": "Alan Silva",
  "license": "MIT"
}
